<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="WrapMatch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="MatchWrap">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="WrapWrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="MatchMatch">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="MatchAuto">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="AutoMatch">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="WrapAuto">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="AutoWrap">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="WrapMatch.Vertical" parent="WrapMatch">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="WrapMatch.Horizontal" parent="WrapMatch">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MatchWrap.Vertical" parent="MatchWrap">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MatchWrap.Horizontal" parent="MatchWrap">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="WrapWrap.Vertical" parent="WrapWrap">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="WrapWrap.Horizontal" parent="WrapWrap">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MatchMatch.Vertical" parent="MatchMatch">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MatchMatch.Horizontal" parent="MatchMatch">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MatchAuto.Vertical" parent="MatchAuto">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="MatchAuto.Horizontal" parent="MatchAuto">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="AutoMatch.Vertical" parent="AutoMatch">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="AutoMatch.Horizontal" parent="AutoMatch">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="WrapAuto.Vertical" parent="WrapAuto">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="WrapAuto.Horizontal" parent="WrapAuto">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="AutoWrap.Vertical" parent="AutoWrap">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="AutoWrap.Horizontal" parent="AutoWrap">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="MatchOne">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>

    <style name="OneMatch">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    
    <style name="Line">
        <item name="android:background">@color/color_D9D9D9</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dip</item>
    </style>
    
    <style name="VLine">
        <item name="android:background">@color/color_D9D9D9</item>
        <item name="android:layout_width">1dip</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    
	<!-- 加载滚动条样式 -->
    <style name="def_loading">
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateDrawable">@drawable/def_loading</item>
        <item name="android:indeterminateDuration">1000</item>
        <item name="android:indeterminateOnly">true</item>
    </style>
</resources>